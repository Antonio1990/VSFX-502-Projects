//// Post Transform User Interface (UI) Mel Script//global proc _PROC_NAME_UI_() {string $selected[] = `ls -sl`;int $i;for( $i=0; $i < size($selected); $i++ ) 	{	string $attr = `rmanGetAttrName "postTransformScript"`;	string $transformName = $selected[$i];		// "Connect" to the mel script that calls 	// Pixar's custom Ri mel procedures.	rmanAddAttr $transformName $attr "_PROC_NAME_RI_";	_ADD_RMAN_ATTR_	}}