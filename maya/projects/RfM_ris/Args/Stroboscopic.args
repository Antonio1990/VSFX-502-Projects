<args format="1.0">
	<shaderType>
		<tag value="projection"/>
	</shaderType>
	<help>
		Multiflash by Patrik Hadron
	</help>
	<param name="numFlashes"
		label="Number of Flashes"
		type="int"
		default="8"
		connectable="False"
		min="1" max="20"
		widget="default">
	</param>
	<rfmdata nodeid="10" classification="RenderMan/projection2"/>
</args>
